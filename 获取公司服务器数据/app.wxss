}

.pop_text {
  padding: 10px;
  background-color: #f4f4f4;
  display: block;
  margin: 20rpx 40rpx 30rpx;
  font-size: 28rpx;
}

.pop-btn {
  display: flex;
  width: 100%;
  height: 44px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.pop-btn .btn {
  display: block;
  -webkit-flex: 1;
  flex: 1;
  height: 44px;
  font-size: 16px;
  color: #fff;
  line-height: 44px;
  text-align: center;
  background-color: #0fa6e8;
  border: none;
  -webkit-box-sizing: inherit;
  box-sizing: inherit;
}

.pop-btn .btn-cancel {
  color: #252825;
  background-color: #fff;
  border-top: 1px solid #e6e6e6;
  border-radius: 0;
  flex: 1;
}

.pop-btn .btn-sure {
  flex: 2;
}

.pop-btn .btn-cancel:after, .pop-btn .btn-sure:after {
  content: none;
}

.pop-btn .btn-default, .pop-btn .btn-sure {
  border-radius: 0 0 0 0;
}
