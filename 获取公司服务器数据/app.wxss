.bm_common:after, .bt_common:before {
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  background-color: #e5e5e5;
  -webkit-transform: scaleY(0.5);
  -webkit-transform-origin: left top;
  z-index: 1;
}

.bt_common:before {
  top: 0;
}

.bm_common:after {
  bottom: -1px;
}

.bt_common, .bm_common {
  position: relative;
}

.bm_common.l-center:after, .bm_common.l-center:before, .bt_common.l-center:after,
.bt_common.l-center:before {
  left: 40rpx !important;
  right: 40rpx !important;
}

.bm_common.l-right:after, .bm_common.l-right:before, .bt_common.l-right:after,
.bt_common.l-right:before {
  left: 40rpx !important;
  right: 0 !important;
}

.bm_common.l-left:after, .bm_common.l-left:before, .bt_common.l-left:after,
.bt_common.l-left:before {
  right: 40rpx !important;
}

.bm_common_dot:after, .bm_common_dot:before {
  background-color: #e5e5e5;
  -webkit-background-size: 15px 15px;
  -moz-background-size: 15px 15px;
  background-size: 15px 15px;
  background-image: -webkit-gradient(radial, 0 100%, 100% 0, color-stop(0.25, rgba(255, 255, 255, 1)), color-stop(0.25, transparent), color-stop(0.5, transparent), color-stop(0.5, rgba(255, 255, 255, 1)), color-stop(0.75, rgba(255, 255, 255, 1)), color-stop(0.75, transparent), to(transparent));
  background-image: -moz-linear-gradient(90deg, rgba(255, 255, 255, 1) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 1) 50%, rgba(255, 255, 255, 1) 75%, transparent 75%, transparent);
  background-image: -o-linear-gradient(90deg, rgba(255, 255, 255, 1) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 1) 50%, rgba(255, 255, 255, 1) 75%, transparent 75%, transparent);
  background-image: linear-gradient(90deg, rgba(255, 255, 255, 1) 25%, transparent 25%, transparent 50%, rgba(255, 255, 255, 1) 50%, rgba(255, 255, 255, 1) 75%, transparent 75%, transparent);
}

.bm-e5:after {
  background-color: rgb(226, 226, 226) !important;
  position: absolute;
  right: 0;
  bottom: 0;
  left: 15px;
  height: 1px;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
}

