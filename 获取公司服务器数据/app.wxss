.popup-mobile {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 199;
  display: none;
  width: 86%;
  background-color: #fff;
  border-radius: 8px;
  overflow: hidden;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
}

.popup-mobile.active {
  display: block;
}

.popup-mobile .close {
  position: absolute;
  width: 30px;
  height: 30px;
  line-height: 30px;
  border-radius: 30px;
  background: #f4f4f4;
  right: -15px;
  top: -15px;
  text-align: center;
  font-size: 20px;
  font-weight: 200;
  color: #666;
}

.img_boda image {
