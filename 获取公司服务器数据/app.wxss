.modal {
  position: fixed;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
  bottom: 0rpx;
  width: 100%;
  height: 100%;
  z-index: 100;
}

.modal-mask {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 97;
  background-color: rgba(0, 0, 0, 0.30);
}

.modal-layer-sm {
  width: 60%;
  transform: translate3d(-50%, -50%, 0);
  left: 50%;
}

.modal-layer-md {
  width: 80%;
  transform: translate3d(-50%, -50%, 0);
  left: 50%;
}

.modal-layer-full {
  width: 100%;
  left: 0;
}

.modal-layer {
  position: absolute;
  background: transparent;
  top: 50%;
  display: flex;
  flex-direction: column;
  z-index: 98;
  box-shadow: 0 4rpx 14rpx rgba(0, 0, 0, 0.4);
  border-radius: 10rpx;
  overflow: hidden;
}

.modal-header {
  background: #fff;
  color: #333;
  padding: 20rpx;
  font-size: 30rpx;
  text-align: center;
}

.modal-body {
  flex: 1;
  padding: 0 40px 40rpx;
  background: #fff;
}

.modal-footer {
  background: #fff;
  flex-direction: row;
  display: flex;
  align-items: center;
  width: 100%;
  border-top: 1rpx solid #eee;
}

.modal-close {
  color: #fff;
  font-size: 48rpx;
  position: absolute;
  right: 40rpx;
  top: 0;
  z-index: 98;
}

.modal .btn {
  flex: 1;
  text-align: center;
  font-size: 30rpx;
  color: #666;
  padding: 19rpx 5rpx;
}

.modal .btn {
  border-right: 1px solid #eee;
}

.modal .btn:last-child {
  border-right: none;
}

.modal .btn-primary {
  color: #2687e5;
}

.search-flex {
  padding: 20rpx;
  position: relative;
  z-index: 13;
  padding-bottom: 0;
  /* transform:  translateY(-100%); */
  margin-top: 0;
  transition: all 0.3s;
}

.search-flex.tophide {
  margin-top: -117rpx;
}

.search-flex button {
  background: #f7982a;
  color: #fff;
  line-height: 72rpx;
  height: 72rpx;
  font-size: 30rpx;
  border-radius: 6rpx;
}

.search-bar {
  flex: 1;
  display: flex;
  border: 1rpx solid #e8e8e8;
  border-radius: 38rpx;
  overflow: hidden;
  background-color: #f4f4f4;
  justify-content: center;
}

.search-bar input, .search-bar .input {
  width: calc(100% - 80rpx);
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 40rpx;
}

.search-bar .search-view {
  display: -webkit-flex;
  display: -webkit-box;
  display: flex;
  align-items: center;
}

.search-bar .input text {
  color: #999;
}

.search-bar .input text:last-child {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 14px;
}

.search-extra-btn {
  margin-left: 20rpx;
  white-space: nowrap;
}

