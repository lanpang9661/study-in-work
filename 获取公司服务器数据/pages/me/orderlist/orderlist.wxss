  align-items: center;
  padding: 0;
  margin-top: 20rpx;
}
.weui_home_h2 .tit{ margin-right: 10rpx}
.weui-panel__bd { margin-bottom: 10px}
.weui-media-box__title-black {
  margin-top: -4px;
}

.list-btn {
position:absolute;
right:0;
bottom:0;
z-index:1;
padding:30px 20px 20px 30px;


}
.list-btn view{font-size: 24rpx;
  border: 1px solid #ccc;
  color: #1f5f9d;
  border-radius: 36rpx;
  text-align: center;
  padding: 2px 10px;
  margin: 0 auto}
  
.list_none image{
  width: 195rpx;
  height: 198rpx;
  margin: 142rpx auto 24rpx auto;
  display: block;
}
.list_none_text{
  width: 100%;
  line-height: 42rpx;
  font-size:30rpx;
  color:rgba(102,102,102,1);
  text-align: center;
}