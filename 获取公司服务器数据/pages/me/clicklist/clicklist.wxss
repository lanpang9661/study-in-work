/* pages/me/collection/collection.wxss */

page {
  background: #f0f0f0;
}

.item {
  position: relative;
}

.weui-panel__bd {
  z-index: 5;
  transition: left 0.2s ease-in-out;
  position: relative;
  top: 0;
  background: #fff;
  width: 100%;
}

.del {
  background: rgba(230, 67, 64, 1);
  width: 208rpx;
  text-align: center;
  font-size: 32rpx;
  z-index: 4;
  right: 0;
  color: #fff;
  position: absolute;
  top: 0;
  height: 100%;
}

.del text {
  position: absolute;
  width: 208rpx;
  height: 50rpx;
  line-height: 50rpx;
  top: 50%;
  margin-top: -25rpx;
  left: 0;
}

.weui-media-box {
  padding: 32rpx;
  border-bottom: 1rpx solid #f0f0f0;
  display: block;
  overflow: hidden;
}

.weui-media-box__hd_in-appmsg {
  width: 250rpx;
  height: 188rpx;
  line-height: 188rpx;
  margin-right: 24rpx;
}

.weui-icon-search_in-box {
  top: 11px;
}

.weui_home_em {
  padding: 2rpx 6rpx;
  font-size: 22rpx;
  float: left;
  margin-right: 16rpx;
  border-radius: 4rpx;
  margin-top: 13rpx;
}

.weui-media-box__bd .weui_home_em:nth-child(3n+2) {
  background: rgba(230, 242, 251, 1);
  color: rgba(81, 170, 253, 1);
}

.weui-media-box__bd .weui_home_em:nth-child(3n+3) {
  background: rgba(253, 242, 224, 1);
  color: rgba(245, 170, 68, 1);
}

.weui-media-box__bd .weui_home_em:nth-child(3n+4) {
  background: rgba(225, 243, 237, 1);
  color: rgba(54, 190, 135, 1);
}

.weui-media-box__title {
  color: #333;
  font-size: 32rpx;
  line-height: 45rpx;
  font-weight: bold;
  white-space: inherit;
  max-height: 90rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.weui_home_image {
  width: 274rpx;
  position: relative;
  float: left;
}

.weui-media-box__bd_in-appmsg {
  width: 400rpx;
  float: left;
}

.weui_home_h2 {
  color: #999;
  font-size: 24rpx;
  line-height: 32rpx;
  padding: 16rpx 0;
}

.weui_home_con {
  float: left;
  margin-right: 40rpx;
}

.item-image-tag {
  width: 30rpx;
  height: 32rpx;
  float: left;
}

.item-image-tag1 {
  width: 28rpx;
  height: 31rpx;
}

.weui_home_con_nub {
  color: #999;
  font-size: 22rpx;
  line-height: 40rpx;
  padding-left: 8rpx;
  float: left;
}

.weui_home_con_bottom {
  overflow: hidden;
  clear: both;
}

.weui_home_con_bottom_text1 {
  color: #f87100;
  font-size: 32rpx;
  line-height: 45rpx;
  float: left;
  margin-right: 40rpx;
}

.weui_home_con_bottom_text2 {
  color: #999;
  font-size: 24rpx;
  line-height: 33rpx;
  float: left;
  padding-top: 10rpx;
}

.weui-loadmore {
  background: #f0f0f0;
  color: #999;
  font-size: 24rpx;
  margin: 0;
  padding: 32rpx 0;
  width: 100%;
  text-align: center;
}

.weui-loadmore__tips_in-line {
  top: 0;
  background: #f0f0f0;
}

.efault_main {
  width: 250rpx;
  height: 188rpx;
  float: left;
  margin-right: 24rpx;
  position: relative;
}

.efault_img {
  width: 250rpx;
  height: 188rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  background: #f0f0f0;
}

.efault_img image {
  display: block;
  width: 74rpx;
  height: 22rpx;
  margin: 83rpx auto 0 auto;
}

.weui-media-box__thumb {
  position: absolute;
  left: 0px;
  top: 0px;
}

.weui-media-box__title-black {
  color: #333;
  font-size: 32rpx;
  line-height: 45rpx;
  font-weight: bold;
  white-space: inherit;
  max-height: 90rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.list_none image {
  width: 195rpx;
  height: 198rpx;
  margin: 142rpx auto 24rpx auto;
  display: block;
}

.list_none_text {
  width: 100%;
  line-height: 42rpx;
  font-size: 30rpx;
  color: rgba(102, 102, 102, 1);
  text-align: center;
}

.h2_label {
  width: 64rpx;
  height: 29rpx;
  line-height: 30rpx;
  text-align: center;
  border: 2rpx solid #f87100;
  border-radius: 2px;
  color: #f87100;
  font-weight: bold;
  font-size: 20rpx;
  background: #fff;
  display: inline-block;
  position: relative;
  top: -5rpx;
}

.ico_1 {
  width: 39rpx;
  height: 65rpx;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}

.weui_home_day {
  overflow: hidden;
  width: 100%;
  font-size: 24rpx;
  color: #999;
  padding-top: 8rpx;
}

.clear {
  clear: both;
}

.yong {
  clear: both;
  font-size: 24rpx;
  background: rgba(245, 245, 245, 1);
  color: rgba(248, 113, 0, 1);
  line-height: 40rpx;
  border-radius: 3rpx;
  overflow: hidden;
  position: relative;
  padding: 8rpx 0;
}

.yong text {
  float: left;
  display: block;
  width: 620rpx;
  margin-left: 55rpx;
}

.mar_bom_15 {
  margin-bottom: 15rpx;
}

.yong image {
  display: block;
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 50%;
  margin-top: -16rpx;
  left: 10rpx;
}

.tishi {
  position: absolute;
  top: 0rpx;
  right: 24rpx;
  width: 100rpx;
  height: 72rpx;
  background: rgba(0, 0, 0, 0.5);
  text-align: center;
  line-height: 28rpx;
  padding-top: 10rpx;
}

.tishi_1 {
  font-size: 34rpx;
  color: #fff;
  display: inline-block;
}

.tishi_2 {
  font-size: 18rpx;
  color: #fff;
  display: inline-block;
}

.tishi_3 {
  font-size: 20rpx;
  color: #fff;
  display: block;
}

.tishi1 .tishi_1, .tishi1 .tishi_2 {
  display: none;
}

.tishi1 .tishi_3 {
  padding-top: 16rpx;
}
