}.delete-btn {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  background-color: #fff;
  color: #333;
  font-size: 32rpx;
  padding: 14rpx 60rpx;
  box-shadow: 0 0 10px #aaa;
  border-radius: 2px;
  z-index: 2
}