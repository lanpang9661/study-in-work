/* pages/passwordDetail/index.wxss */
page{
  background:rgba(240,240,240,1);
  position: relative;
  top: -1rpx;
}
.baner{
  position: relative;
  height: 560rpx;
}
.baner image{
  width: 100%;
  height: 560rpx;
}
.baner_h2{
  background:linear-gradient(180deg,rgba(0,0,0,0),rgba(0,0,0,0.5));
  height: 88rpx;
  line-height: 100rpx;
  padding: 0 32rpx;
  color: #fff;
  font-size: 28rpx;
  position: absolute;
  bottom:0;
  width: 686rpx;
}
.lock_layer1{
  background: #fff;
  padding: 32rpx;
}
.lock_layer1_top{
  width: 100%;
}
.lock_layer1_top_name{
  color:rgba(102,102,102,1);
  font-size: 34rpx;
  font-weight: bold;
  line-height: 45rpx;
  display:inline-block;
}
.lock_layer1_top_num{
  color:rgba(248,113,0,1);
  font-size: 34rpx;
  line-height: 45rpx;
  padding-left: 8rpx;
  display:inline-block;
}
.lock_layer1_bottom{
  color:rgba(51,51,51,0.85);
  font-size: 29rpx;
  line-height: 39rpx;
  margin-top: 16rpx;
}
.lock_layer1_bottom_name{
  display:inline-block;
}
.lock_layer1_bottom_num{
  display:inline-block;
}
.lock_layer2{
  margin-top: 24rpx;
}
.weui-cells{
  position:initial;
}
.weui-cell{
  padding:0rpx 40rpx;
  height: 106rpx;
  line-height: 106rpx;
  position:initial;
  border-bottom:1px solid #F0F0F0;
}
.weui-cell__bd{
  color:rgba(51,51,51,0.85);
  font-size:32rpx;
}
.weui-cell__ft{
  color:rgba(153,153,153,0.85);
  font-size: 24rpx;
}
.lock_layer2_hd{
  height: 1rpx;
  width: 100%;
  background: #fff;
  position: relative;
  bottom:-1rpx;
}
.efault{
  width: 100%;
  height: 560rpx;
}
.efault_img{
  width: 100%;
  height: 560rpx;
  position: absolute;
  left: 0rpx;
  top: 0rpx;
  background: #F0F0F0;
}
.efault_img image{
  display: block;
  width: 159rpx;
  height: 47rpx;
  margin: 255rpx auto 0 auto;
}
.efoault_img_con{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}