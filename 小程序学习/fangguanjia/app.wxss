.sell-inform-mask {
  display: none;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  z-index: 100;
}

.popup-mobile {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 199;
  display: none;
  width: 86%;
   -webkit-transform: translate3d(-50%, -50%, 0);
  transform: translate3d(-50%, -50%, 0);
  background-color: #fff;
  border-radius: 8px;
  overflow: hidden;
}

.popup-mobile.active {
  display: block;
}

.popup-mobile .close {
  position: absolute;
  width: 30px;
  height: 30px;
  line-height: 30px;
  border-radius: 30px;
  background: #f4f4f4;
  right: -15px;
  top: -15px;
  text-align: center;
  font-size: 20px;
  font-weight: 200;
  color: #666;
}

.pop-error, .pop-ipt, .pop-tel, .pop-text, .popup-mobile .pop-tit {
  padding-left: 20px;
  padding-right: 20px;
}

.popup-mobile .pop-tit {
  height: 50px;
