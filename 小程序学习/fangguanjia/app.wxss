.bottom_layer {
  position: fixed;
  z-index: 100;
  text-align: center;
  width: 100%;
  bottom: 0;
  background: #fff;
  -webkit-transform: translateY(100%);
  -webkit-transition: all 0.2s ease;
  transform: translateY(100%);
  transition: all 0.2s ease;
}

.bottom_layer.active {
  -webkit-transform: translateY(0);
  transform: translateY(0);
}

.bottom_layer .title {
  line-height: 0.5rem;
  border-bottom: 1px solid #ccc;
}

.bottom_layer .cancle {
  position: absolute;
  color: #9c9fa1;
  right: 0.16rem;
}

.bottom_layer .title p {
  text-align: center;
  font-size: 0.18rem;
  font-weight: 700;
}

.bottom_layer .option_wrap {
  max-height: 2rem;
  overflow: scroll;
}

.bottom_layer li {
  margin-left: 0.0256rem;
  height: 0.5rem;
  text-align: center;
  text-indent: -0.0256rem;
}

.layer_fixed, .layer_static {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.layer_fixed {
  width: 100%;
